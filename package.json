{
  "name": "@jupiterone/graph-whois",
  "version": "0.1.2",
  "description": "A JupiterOne integration that ingests domain information from Whois service",
  "main": "dist/index.js",
  "repository": "https://github.com/JupiterOne/graph-whois",
  "license": "SEE LICENSE IN LICENSE",
  "files": [
    "LICENSE",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "ts-node --require dotenv/config tools/execute.ts | bunyan",
    "start:containers": "docker-compose up -d",
    "stop:containers": "docker-compose down",
    "restart:containers": "docker-compose restart",
    "prebuild": "yarn test",
    "build": "rm -rf dist/* && yarn build:types && yarn build:src && yarn build:docs",
    "build:src": "babel src --extensions '.ts' --out-dir dist",
    "build:docs": "ts-node tools/docs.ts",
    "build:types": "tsc --project tsconfig.dist.json --declaration --emitDeclarationOnly",
    "lint": "yarn lint:es && yarn lint:md",
    "lint:md": "remark .",
    "lint:es": "eslint . --ext .ts --ignore-pattern dist",
    "format": "yarn prettier --write '**/*.{ts,js,json,md}'",
    "test": "jest"
  },
  "dependencies": {
    "@lifeomic/attempt": "^3.0.0",
    "is-valid-domain": "^0.0.11",
    "whois-api": "^1.0.2"
  },
  "peerDependencies": {
    "@jupiterone/jupiter-managed-integration-sdk": "^24.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "@jupiterone/jupiter-managed-integration-sdk": "^24.2.0",
    "@types/bunyan": "^1.8.5",
    "@types/fs-extra": "^7.0.0",
    "@types/jest": "^24.0.0",
    "@types/node": "~8.10.0",
    "bunyan": "^1.8.12",
    "dotenv": "^8.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "fs-extra": "^8.0.1",
    "husky": "^2.3.0",
    "jest": "^24.0.0",
    "jest-haste-map": "^24.0.0",
    "jest-resolve": "^24.1.0",
    "lint-staged": "^8.1.3",
    "prettier": "1.18.0",
    "remark-cli": "^6.0.1",
    "remark-lint": "^6.0.4",
    "remark-preset-lint-consistent": "^2.0.2",
    "remark-preset-lint-markdown-style-guide": "^2.1.2",
    "remark-preset-lint-recommended": "^3.0.2",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5"
  }
}
